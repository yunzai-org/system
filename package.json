{
  "name": "yz-system",
  "version": "1.0.9",
  "author": "ningmengchongshui",
  "description": "yz-system",
  "type": "module",
  "scripts": {
    "app": "npx tsx src/main.js",
    "dev": "nodemon",
    "start": "pm2 startOrRestart pm2.config.cjs",
    "stop": "pm2 stop pm2.config.cjs",
    "delete": "pm2 delete pm2.config.cjs",
    "kill": "pm2 kill",
    "logs": "pm2 logs",
    "monit": "pm2 monit",
    "img-app": "npx tsx routes.server.tsx",
    "img-dev": "nodemon --config routes.nodemon.json",
    "build": "rollup  --config rollup.config.js",
    "format": "prettier --write .",
    "check-format": "git diff --exit-code",
    "prepare": "husky"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "moment": "^2.30.1",
    "puppeteer": "^23.1.1",
    "react": "^18.3.1",
    "react-puppeteer": "^1.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.8.5",
    "@types/node-schedule": "^2.1.7",
    "@types/react": "^18.3.4",
    "husky": "^9.1.5",
    "prettier": "^3.0.3",
    "rollup": "^4.16.4",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "yunzai": "^1.0.6"
  },
  "types": "lib/types",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/types/index.d.ts"
    }
  },
  "keywords": [
    "yunzai"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=16.14.0"
  }
}
